{
  "version": 2,
  "builds": [
    {
      "src": "api/process-music.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/process-music",
      "dest": "/api/process-music.py",
      "methods": ["POST", "OPTIONS", "GET"]
    },
    {
      "src": "/",
      "dest": "/api/process-music.py",
      "methods": ["GET"]
    }
  ]
}
